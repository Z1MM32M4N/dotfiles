{
    "update.mode": "none",
    "security.workspace.trust.enabled": false,
    "telemetry.telemetryLevel": "off",
    "workbench.startupEditor": "none",
    "editor.fontFamily": "Iosevka Fixed, Menlo, Monaco, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.renderLineHighlight": "none",
    "editor.guides.indentation": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.currentLine.enabled": false,
    "gitlens.currentLine.pullRequests.enabled": false,
    "gitlens.codeLens.enabled": false,
    "redhat.telemetry.enabled": false,
    "files.insertFinalNewline": true,
    "editor.renderFinalNewline": "off",
    "remote.autoForwardPorts": false,
    "remote.restoreForwardedPorts": false,
    "remote.SSH.enableAgentForwarding": false,
    "remote.SSH.path": "sc-ssh-wrapper",
    "remote.SSH.useLocalServer": true,
    "debug.javascript.automaticallyTunnelRemoteServer": false,
    "update.channel": "none",
    "workbench.preferredDarkColorTheme": "Better Solarized Dark",
    "workbench.preferredLightColorTheme": "Better Solarized Light",
    "window.autoDetectColorScheme": true,
    "telemetry.enableTelemetry": false,
    "typescript.tsdk": "frontend/js-scripts/node_modules/typescript/lib",
    "search.smartCase": true,
    "remote.SSH.connectTimeout": 60,
    "C_Cpp.intelliSenseEngine": "Disabled",
    "window.title": "${appName}${separator}${rootPath}${separator}${activeEditorMedium}",
    "editor.minimap.enabled": false,
    "vale.doNotShowWarningForFileToBeSavedBeforeLinting": true,
    "run-pay.liveNudging.enabled": false,
    "github.copilot.enable": {
      "*": false,
      "ruby": true,
      "java": true,
      "python": true,
      "go": true,
      "scala": true,
      "javascript": true,
      "typescript": true
    },
    "search.followSymlinks": false,
    "window.zoomLevel": 1,
    "workbench.editor.tabCloseButton": "left",
    "workbench.colorTheme": "Better Solarized Light",
}
